+    private void clearKeys() {
+        SharedPreferences prefs = getSharedPreferences(
+                Constants.ACCOUNT_PREFS_NAME, 0);
+        Editor edit = prefs.edit();
+        edit.clear();
+        edit.commit();
+    }
